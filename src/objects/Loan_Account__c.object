IF(AND(Number_of_Days_Overdue__c&gt;120,Number_of_Days_Overdue__c&lt;=180),&quot;121-180&quot;,
IF(Number_of_Days_Overdue__c&gt;180,&quot;180+&quot;,
&quot;&quot;))))))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
