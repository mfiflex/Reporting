    <validationRules>
        <fullName>Check_Accounting_Type</fullName>
        <active>true</active>
        <errorConditionFormula>(Accrual_Based_Accounting__c &amp;&amp;  Cash_Based_Accounting__c)
||
(NOT(Accrual_Based_Accounting__c) &amp;&amp;  NOT(Cash_Based_Accounting__c))</errorConditionFormula>
        <errorMessage>Both accrual and cash based accounting cannot be checked or unchecked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Loan_Cycle_Setup_For_Amount</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  ISPICKVAL(Loan_Cycle_Setup__c, &apos;Same for all Loans&apos;) ,OR( ISBLANK( Max_Loan_Amount__c ) , ISBLANK( Min_Loan_Amount__c ) ) )</errorConditionFormula>
        <errorMessage>Please fill out the Loan section.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Loan_Cycle_Setup_For_Installment</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  ISPICKVAL(Loan_Cycle_Setup__c, &apos;Same for all Loans&apos;) ,OR( ISBLANK(  Minimum_Gap_Between_Installments__c ) , ISBLANK(  Maximum_Gap_Between_Installments__c ), ISBLANK(  Max_Number_of_Installments__c ) , ISBLANK(  Min_Number_of_Installments__c ) , ISBLANK(  Minimum_Installment_Amount__c ),  ISBLANK( Default_Number_of_Installments__c )   ) )</errorConditionFormula>
        <errorMessage>Please fill out the Installment section.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Loan_Cycle_Setup_For_Interest</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  ISPICKVAL(Loan_Cycle_Setup__c, &apos;Same for all Loans&apos;) ,OR( ISBLANK( Max_Interest_Rate__c ) , ISBLANK( Min_Interest_Rate__c ), ISBLANK( Max_Overdue_Interest_Rate__c ) , ISBLANK( Max_Overdue_Interest_Rate__c ) , ISBLANK( Default_Interest_Rate__c )  ) )</errorConditionFormula>
        <errorMessage>Please fill out the Interest section.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Product_Name</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(Name) || ISNULL(Name)</errorConditionFormula>
        <errorMessage>Loan Product name cannot be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Unique_Loan_Product</fullName>
        <active>true</active>
        <errorConditionFormula>Name = VLOOKUP( $ObjectType.Loan_Product__c.Fields.Name , $ObjectType.Loan_Product__c.Fields.Name , Name ) 
&amp;&amp; 
Id &lt;&gt; VLOOKUP( $ObjectType.Loan_Product__c.Fields.Id , $ObjectType.Loan_Product__c.Fields.Name , Name )</errorConditionFormula>
        <errorMessage>Loan Product Name already exists.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Count_Loan_Approval_Prereqs</fullName>
        <active>true</active>
        <errorConditionFormula>Number_Of_Prereqs__c &gt; 1</errorConditionFormula>
        <errorMessage>You can only associate one Loan Approval Prerequisite with a Loan Product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Interest_Only_Product_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISPICKVAL( Interest_Calculation_Method__c , &apos;Interest Only&apos;), 
ISBLANK(Interest_Only_Period__c) 
)</errorConditionFormula>
        <errorMessage>&apos;Interest Only Period&apos; must be provided for an &apos;Interest Only&apos; product.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Interest_Rate_Boundary_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Loan_Cycle_Setup__c, &apos;Same for all Loans&apos;),(Default_Interest_Rate__c &gt; Max_Interest_Rate__c
||
Default_Interest_Rate__c &lt; Min_Interest_Rate__c))</errorConditionFormula>
        <errorMessage>The Default Interest Rate should fall between the specified Limits.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_of_Installments_Boundary_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Loan_Cycle_Setup__c, &apos;Same for all Loans&apos;),( Default_Number_of_Installments__c &gt; Max_Number_of_Installments__c 
|| 
Default_Number_of_Installments__c &lt; Min_Number_of_Installments__c ))</errorConditionFormula>
        <errorDisplayField>Default_Number_of_Installments__c</errorDisplayField>
        <errorMessage>The default No. of Installments should fall between the specified Limits.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Overdue_Interest_Rate_Boundary_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Loan_Cycle_Setup__c, &apos;Same for all Loans&apos;),(Default_Overdue_Interest_Rate__c &gt; Max_Overdue_Interest_Rate__c
||
Default_Overdue_Interest_Rate__c &lt; Min_Overdue_Interest_Rate__c))</errorConditionFormula>
        <errorMessage>The Default Overdue Interest Rate should fall between the specified Limits.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Savings_Deposit_type_must_be_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Savings_Product__r.Type_of_Deposits__c, &quot;Voluntary&quot;)</errorConditionFormula>
        <errorMessage>Savings Must be Mandatory. Check linked Savings Product definition.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Savings_Product_must_be_active</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Savings_Product__r.Status__c , &quot;Inactive&quot;)</errorConditionFormula>
        <errorMessage>Savings Product must be active. Please check its status.</errorMessage>
    </validationRules>
