        <errorConditionFormula>AND( (Deposit_Start_Date__c &lt;  TODAY()),( PRIORVALUE(Deposit_Start_Date__c)  &lt;&gt; Deposit_Start_Date__c  ))</errorConditionFormula>
        <errorDisplayField>Deposit_Start_Date__c</errorDisplayField>
        <errorMessage>Start Date can&apos;t be in the past.</errorMessage>
    </validationRules>
