    <validationRules>
        <fullName>Address1_Special_char_check</fullName>
        <active>true</active>
        <description>Address1 Special_char_check</description>
        <errorConditionFormula>NOT(REGEX( Address_1__c , &quot;^[a-zA-Z0-9-&amp;/#\\s]*$&quot;))</errorConditionFormula>
        <errorDisplayField>Address_1__c</errorDisplayField>
        <errorMessage>Address should not include special characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Address2_Special_char_check</fullName>
        <active>false</active>
        <description>Address Special_char_check</description>
        <errorConditionFormula>NOT(REGEX( Address_2__c , &quot;^[a-zA-Z0-9-&amp;/#\\s]*$&quot;))</errorConditionFormula>
        <errorDisplayField>Address_2__c</errorDisplayField>
        <errorMessage>Address should not include special characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Address3_Special_char_check</fullName>
        <active>false</active>
        <description>Address Special_char_check</description>
        <errorConditionFormula>NOT(REGEX( Address_3__c , &quot;^[a-zA-Z0-9-&amp;/#\\s]*$&quot;))</errorConditionFormula>
        <errorDisplayField>Address_3__c</errorDisplayField>
        <errorMessage>Address should not include special characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Age_Between_18_and_60</fullName>
        <active>true</active>
        <errorConditionFormula>OR
      ((IF(
             MONTH(TODAY())&gt;MONTH( Date_of_Birth__c ),
             YEAR(TODAY())-YEAR(Date_of_Birth__c ),
             IF(AND(MONTH(TODAY()) = MONTH(Date_of_Birth__c ), DAY(TODAY()) &gt;=      
             DAY(Date_of_Birth__c )),YEAR(TODAY())-YEAR(Date_of_Birth__c),
             (YEAR(TODAY())-YEAR(Date_of_Birth__c ))-1)
             )
             &lt;  18) ,
       (IF(
            MONTH(TODAY())&gt;MONTH( Date_of_Birth__c ),
            YEAR(TODAY())-YEAR(Date_of_Birth__c ),
            IF(AND(MONTH(TODAY()) = MONTH(Date_of_Birth__c ), DAY(TODAY()) &gt;= 
            DAY(Date_of_Birth__c )),YEAR(TODAY())-YEAR(Date_of_Birth__c),
           (YEAR(TODAY())-YEAR(Date_of_Birth__c ))-1)
           )
           &gt;   60))</errorConditionFormula>
        <errorDisplayField>Date_of_Birth__c</errorDisplayField>
        <errorMessage>Age should be between18 to 60 years</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Center_Check</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK(Center__c)) &amp;&amp; NOT(ISBLANK( Group__c )) &amp;&amp; ( Group__r.Center__c !=  Center__c || ISBLANK(Group__r.Center__c))</errorConditionFormula>
        <errorMessage>User entered Center does not match Center for Group</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_Max_Number_of_Persons</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( ISBLANK( Group__c ) ) &amp;&amp;  (Group__r.Maximum_number_of_persons__c  =  Group__r.Number_of_Active_Clients__c )</errorConditionFormula>
        <errorMessage>You cannot add more clients to this group. The allowable number of clients has already been reached.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>City_Special_char_check</fullName>
        <active>false</active>
        <description>City_Special_char_check</description>
        <errorConditionFormula>NOT(REGEX( Address_3__c , &quot;^[a-zA-Z\\s]*$&quot;))</errorConditionFormula>
        <errorDisplayField>City__c</errorDisplayField>
        <errorMessage>City should not include special characters</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ClientStartDate</fullName>
        <active>false</active>
        <description>This will not work here. Will only work in a trigger. Because otherwise you can never make any changes to a client! E.g. add a fee set. So deactivating for now.</description>
        <errorConditionFormula>Client_Start_Date__c  &lt;    TODAY()</errorConditionFormula>
        <errorMessage>The client&apos;s start date cannot be before today.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EitherCenterOrGroupCheck</fullName>
        <active>false</active>
        <description>The client&apos;s Loan Officer and the Group&apos;s/Center&apos;s Loan Officer must be the same.</description>
        <errorConditionFormula>((Group__c &lt;&gt; null) &amp;&amp; ( Loan_Officer__r.Id   &lt;&gt; Group__r.Assigned_LO__r.Id))  || (( Center__c  &lt;&gt; null) &amp;&amp; ( Loan_Officer__r.Id   &lt;&gt;  Center__r.Loan_Officer__r.Id ))</errorConditionFormula>
        <errorMessage>The client&apos;s Loan Officer and the Group&apos;s/Center&apos;s Loan Officer must be the same.</errorMessage>
    </validationRules>
